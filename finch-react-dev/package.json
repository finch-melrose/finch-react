{
  "name": "react-native-web-dev",
  "version": "0.0.1",
  "scripts": {
    "start": "react-native start",
    "web-dev": "node web/start-dev-server",
    "web-bundle": "NODE_ENV=production webpack --config web/webpack.config.js --colors",
    "server": "npm run web-bundle && npm run server-bundle && WEB_BUNDLE='./web/output/' node server/output/bundle.js",
    "server-only": "npm run server-bundle && WEB_BUNDLE='./web/output/' node server/output/bundle.js",
    "server-bundle": "NODE_ENV=production webpack --config server/webpack.config.js --colors"
  },
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "compression-webpack-plugin": "^0.2.0",
    "finch-react-core": "../finch-react-core",
    "finch-react-web": "../finch-react-web",
    "json-loader": "^0.5.2",
    "lodash": "^4.0.0",
    "normalize.css": "^3.0.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-native": "^0.18.0",
    "react-native-cli": "^0.1.10",
    "react-web": "^0.2.1"
  },
  "devDependencies": {
    "babel-loader": "^6.2.1",
    "css-loader": "^0.23.1",
    "fbjs": "^0.6.1",
    "haste-resolver-webpack-plugin": "^0.1.2",
    "node-libs-browser": "^1.0.0",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.13.0",
    "underscore": "^1.8.3",
    "webpack": "^1.9.10",
    "webpack-dev-server": "^1.9.0",
    "webpack-html-plugin": "^0.1.1"
  }
}
